        partialRootPath = EXT:cf_cookiemanager/Resources/Private/Backend/Partials/
        # cat=module.tx_cfcookiemanager_cookiesettings/file; type=string; label=Path to template layouts (BE)
        layoutRootPath = EXT:cf_cookiemanager/Resources/Private/Backend/Layouts/
    }
    persistence {
        # cat=module.tx_cfcookiemanager_cookiesettings//a; type=string; label=Default storage PID
        storagePid =
    }
}

